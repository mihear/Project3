<script src="~/newchart/jquery-3.1.1.min.js"></script>

<style>
    #container {
        height: 400px;
        min-width: 310px;
        max-width: 800px;
        margin: 0 auto;
    }
</style>
<div id="container" style="height: 400px"></div>

<script>
    var y = [];
    var chart = new Highcharts.chart('container', {
        chart: {
            type: 'column',
            options3d: {
                enabled: true,
                alpha: 10,
                beta: 25,
                depth: 70
            }
        },
        title: {
            text: '3D chart with null values'
        },
        subtitle: {
            text: 'Notice the difference between a 0 value and a null point'
        },
        plotOptions: {
            column: {
                depth: 25
            }
        },
        xAxis: {
            categories:y,
            //['dish1', 'dish1', 'dish1', 'dish1', 'dish1', 'dish1', 'dish1', 'dish1', 'dish1', 'dish1'],
            labels: {
                skew3d: true,
                style: {
                    fontSize: '16px'
                }
            }
        },
        //yAxis: {
        //    categories: [100, 200, 400, 800, 1000, 1200, 1400, 1800],
        //    title: {
        //        text: null
        //    }
        //},
        //series: [{
        //    name: 'Dishs',
        //    data: [2, 3, 6, 4, 4, 5, 1, 4, 6, 3]
        //}]
    });
    $.ajax({
        url: '/api/DimDishes',
        type: 'GET',
        async: true,
        dataType: "json",
        success: function (data) {
            var d = [];
            for (var i = 0 ; i < data.length; i++) {
                d.push(data[i].data);
                y.push(data[i].name);
            }
            //alert(d);
            chart.addSeries({
                name: "Count",
                data: d
            });
        }
    });
</script>
